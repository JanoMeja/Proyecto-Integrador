		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar productos</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<form action="/users/profile" method="GET" class="registro">
                <label for="imagen">Imagen</label>
                <input type="file" id="archivo" name="archivo" accept=".pdf,.doc,.docx">

                <label for="usuario">Nombre del Producto:</label>
                <input type="text" id="usuario" name="usuario"><br><br>

				<label for="descripcion">Descripci√≥n:</label>
                <input type="text" id="descripcion" name="descripcion"><br><br>

                <label for="fecha_carga">Fecha de carga:</label>
                <input type="date" id="fecha_carga" name="fecha_carga"><br><br>

                <input type="submit" value="Agregar Producto">
            </form>
		</div>
	</div>
        <%- include('./partials/footer') %>